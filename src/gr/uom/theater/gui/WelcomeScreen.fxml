<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<StackPane prefHeight="200.0" prefWidth="300.0" styleClass="container" xmlns="http://javafx.com/javafx/8.0.65"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="gr.uom.theater.gui.controllers.WelcomeScreenController">
    <VBox alignment="CENTER" spacing="30.0">
        <HBox alignment="CENTER" spacing="10.0">
            <Label alignment="CENTER_RIGHT" minWidth="110.0" prefWidth="110.0" text="Σύνδεση ως :">
                <font>
                    <Font name="System Bold Italic" size="12.0"/>
                </font>
            </Label>
            <ComboBox prefWidth="150.0" fx:id="cbConnectAs"/>
        </HBox>
        <Button mnemonicParsing="false" text="Είσοδος" fx:id="btnEnter"/>
    </VBox>
</StackPane>
